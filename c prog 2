#include <stdio.h>
 int main(int argc, char** argv)
 {
 	int mark;
	 printf("enter marks");
	 scanf("%d",&mark);
	 if (mark>=70&& mark<=100)
	 {
	 	printf("Grade A");
	 }
	 else if(mark>=60 &&mark<=69)
	 {
	 	printf("Grade B");

	 }
	 else if(mark>=50 &&mark<=59)
	 {
	 	printf("Grade C");
	 }
	 else if(mark>=40 && mark<=49)
	 {
	 	printf("Grade D");
	 }
	 else if(mark<40) 
	 {
	 	printf("grade F");
	 }
	 else 
	 {
	 	printf("Invalid mark");
	 }
	 return 0;
 }
